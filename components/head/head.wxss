/* components/head/head.wxss */
.head{
    background-color: #3190e8;
    position: relative;
    z-index: 100;
    left: 0;
    top: 0;
    width: 100%;
    height: 90rpx;
}
.head .head-icon{
    position: absolute;
    left:18rpx;
    top:50%;
    transform: translateY(-50%);
    width:28rpx;
    height: 46rpx;
}
.head-icon image{
    width:28rpx;
    height: 46rpx;
}
.head .head-title{
    width:100%;
    height: 90rpx;
    text-align: center;
    line-height: 90rpx;
    color:#fff;
    font-size:40rpx;
    font-weight: bold;
}