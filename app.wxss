/**app.wxss**/

@import "./wxParse/wxParse.wxss";
@import './static/css/common.wxss';
page{
  height: 100%;
  background: #fff;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.lineEllipise{
 /* 一行 */
  width:300rpx;
  border:1px solid;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}
.linesEllipise{
  /* 两行 */
text-overflow: -o-ellipsis-lastline;
overflow: hidden;
text-overflow: ellipsis;
display: -webkit-box;
-webkit-line-clamp: 2;
line-clamp: 2;
-webkit-box-orient: vertical;
}